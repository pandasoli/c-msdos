
INC=T:INCLUDE;C:INCLUDE
LIB=T:LIB

OBJS=C:BUILD\MAIN.OBJ C:BUILD\LIB.OBJ

MAIN.EXE : $(OBJS)
	TCC -I$(INC) -L$(LIB) -eC:BUILD\MAIN.EXE $(OBJS)

C:BUILD\MAIN.OBJ : C:SRC\MAIN.c
	TCC -I$(INC) -L$(LIB) -c -oC:BUILD\MAIN.OBJ C:SRC\MAIN.c

C:BUILD\LIB.OBJ : C:SRC\LIB.c
	TCC -I$(INC) -L$(LIB) -c -oC:BUILD\LIB.OBJ C:SRC\LIB.c
