#; vim:ft=make

CC=TCC
CFLAGS=-I$(INC) -L$(LIB) -w

INC=T:INCLUDE;C:INCLUDE
LIB=T:LIB

OBJS=C:BUILD\MAIN.OBJ C:BUILD\LIB.OBJ

MAIN.EXE : $(OBJS)
	$(CC) $(CFLAGS) -eC:BUILD\MAIN.EXE $(OBJS)

C:BUILD\MAIN.OBJ : C:SRC\MAIN.c
	$(CC) $(CFLAGS) -c -oC:BUILD\MAIN.OBJ C:SRC\MAIN.c

C:BUILD\LIB.OBJ : C:SRC\LIB.c
	$(CC) $(CFLAGS) -c -oC:BUILD\LIB.OBJ C:SRC\LIB.c
